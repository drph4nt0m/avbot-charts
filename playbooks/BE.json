{
  "enabled": true,
  "country": {
    "iso": "BE",
    "name": "Belgium"
  },
  "source": "AIM Belgium",
  "scraper": {
    "engine": "axios",
    "features": {
      "baseUrl": "https://ops.skeyes.be/html/belgocontrol_static/eaip/eAIP_Main/html",
      "paths": [
        {
          "route": "/index-en-GB.html",
          "navigations": [
            {
              "selector": "frame[name=\"eAISNavigation\"]",
              "attribute": "src"
            }
          ]
        }
      ],
      "chart": {
        "baseUrl": "${baseUrl}/eAIP/",
        "selector": "a[id=\"AD-2.${icao}\"]",
        "attribute": "href"
      }
    }
  }
}
