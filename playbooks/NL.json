            },
            {
              "selector": "frame[name=\"eAISMenuFrameset\"]",
              "attribute": "src"
            },
            {
              "selector": "frame[name=\"eAISMenuContent\"]",
              "attribute": "src"
            }
          ]
        }
      ],
      "chart": {
        "baseUrl": "${baseUrl}/eAIP/",
        "selector": "a[title=\"${icao}\"]",
        "attribute": "href"
      }
    }
  }
